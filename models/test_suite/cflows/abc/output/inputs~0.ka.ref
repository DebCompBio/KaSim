%def: "seed" "924556145"

%agent: A(x,c)
%agent: B(x)
%agent: C(x1~u~p,x2~u~p)

%var:/*0*/ 'on_rate' 0.0001
%var:/*1*/ 'off_rate' 0.1
%var:/*2*/ 'mod_rate' 1
%var:/*3*/ 'AB' |A(x!1), B(x!1)|
%var:/*4*/ 'Cuu' |C(x1~u?, x2~u?)|
%var:/*5*/ 'Cpu' |C(x1~p?, x2~u?)|
%var:/*6*/ 'Cpp' |C(x1~p?, x2~p?)|
%var:/*7*/ 'n_ab' 1000
%var:/*8*/ 'n_c' 10000
%plot: [T]
%plot: AB
%plot: Cuu
%plot: Cpu
%plot: Cpp

'a.b' A(x/!1), B(x/!1) @ on_rate
'a.b_op' A(x!1/), B(x!1/) @ off_rate
'ab.c' A(x!_, c/!2), C(x1~u/!2) @ on_rate
'mod x1' C(x1~u/~p!1/), A(c!1/) @ mod_rate
'a.c' A(x, c/!1), C(x1~p, x2~u/!1) @ on_rate
'mod x2' A(x, c!1/), C(x1~p, x2~u/~p!1/) @ mod_rate

/*0*/%mod: [true] do $TRACK C(x1~u?, x2~u?) [true]
/*1*/%mod: [true] do $TRACK C(x1~p?, x2~u?) [true]
/*2*/%mod: [true] do $TRACK C(x1~p?, x2~p?) [true]
/*3*/%mod: [true] do $FLUX "flux_map.dot" "absolute" [true]
/*4*/%mod: ([E] = 1999) do $FLUX "flux_map.dot" [false]; $DEL 10000 C(x1~p?, x2~p?)
%init: n_ab A(x, c), B(x)
%init: n_c C(x1~u, x2~u)
