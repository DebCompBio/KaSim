"2:B(x~u)->B(x~p)" -> "0:A(x~u),B(x~p)->A(x~u),A(x~p)" [label="[0->1]" color="#ff0000" arrowhead="tee"];
"2:B(x~u)->B(x~p)" -> "3:B(x~p)->B(x~u)" [label="[0->0]" color="#ff0000" arrowhead="tee"];
"3:B(x~p)->B(x~u)" -> "2:B(x~u)->B(x~p)" [label="[0->0]" color="#ff0000" arrowhead="tee"];
