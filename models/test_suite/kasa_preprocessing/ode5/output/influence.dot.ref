#Download sources/binaries at https://github.com/Kappa-Dev/KaSim
#
#Command line is: KaSa ode5.ka --debugging-mode --output-directory output
#
#This file contains the description of the influence map in dot.
#Please use graphviz (http://www.graphviz.org) or OmniGraffle to export it to a PDF
#
digraph G{
"r0" [shape="box" color="lightskyblue"];
"r1" [shape="box" color="lightskyblue"];
"r2" [shape="box" color="lightskyblue"];
"r3" [shape="box" color="lightskyblue"];
"r4" [shape="box" color="lightskyblue"];
"r0" -> "r2" [label="[1->0]" color="green"];
"r0" -> "r4" [label="[0->1]" color="green"];
"r1" -> "r3" [label="[1->0]" color="green"];
"r1" -> "r4" [label="[0->1]" color="green"];
"r2" -> "r4" [label="[1->0]" color="green"];
"r3" -> "r4" [label="[1->0]" color="green"];

}
