"r3" -| "r7" [label="[1->1]" color="red"];
"r4" -| "r2" [label="[1*->0];[0*->0]" color="red"];
"r4" -| "r3" [label="[1*->1];[0*->1];[0->0]" color="red"];
"r4" -| "r6" [label="[1*->1];[0*->1]" color="red"];
"r5" -| "r6" [label="[0->0]" color="red"];
