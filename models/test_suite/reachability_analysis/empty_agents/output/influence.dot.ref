#Download sources/binaries at https://github.com/Kappa-Dev/KaSim
#
#Command line is: KaSa empty_agents.ka --debugging-mode --output-directory output
#
#This file contains the description of the influence map in dot.
#Please use graphviz (http://www.graphviz.org) or OmniGraffle to export it to a PDF
#
digraph G{
"0:->A()" [shape="box" color="lightskyblue"];
"1:A()->B()" [shape="box" color="lightskyblue"];
"2:B()->C()" [shape="box" color="lightskyblue"];
"3:C()->D(x~u)" [shape="box" color="lightskyblue"];
"4:E()->F(x~u)" [shape="box" color="lightskyblue"];
"0:->A()" -> "1:A()->B()" [label="[0->0]" color="green"];
"1:A()->B()" -> "2:B()->C()" [label="[0->0]" color="green"];
"2:B()->C()" -> "3:C()->D(x~u)" [label="[0->0]" color="green"];

}
