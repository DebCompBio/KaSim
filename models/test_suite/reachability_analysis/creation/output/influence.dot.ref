#Download sources/binaries at https://github.com/Kappa-Dev/KaSim
#
#Command line is: KaSa creation.ka --debugging-mode --output-directory output
#
#This file contains the description of the influence map in dot.
#Please use graphviz (http://www.graphviz.org) or OmniGraffle to export it to a PDF
#
digraph G{
"0:->A(x~u,y~u)" [shape="box" color="lightskyblue"];
"1:A(x~u,y~u)->A(x~p,y~u)" [shape="box" color="lightskyblue"];
"2:A(x~u,y~u)->A(x~u,y~p)" [shape="box" color="lightskyblue"];
"3:A(x~p,y~u)->D(x~u)" [shape="box" color="lightskyblue"];
"4:D(x~u)->D(x~p)" [shape="box" color="lightskyblue"];
"0:->A(x~u,y~u)" -> "1:A(x~u,y~u)->A(x~p,y~u)" [label="[0->0]" color="green"];
"0:->A(x~u,y~u)" -> "2:A(x~u,y~u)->A(x~u,y~p)" [label="[0->0]" color="green"];
"1:A(x~u,y~u)->A(x~p,y~u)" -> "3:A(x~p,y~u)->D(x~u)" [label="[0->0]" color="green"];
"3:A(x~p,y~u)->D(x~u)" -> "4:D(x~u)->D(x~p)" [label="[0->0]" color="green"];

"0:->A(x~u,y~u)" -| "1:A(x~u,y~u)->A(x~p,y~u)" [label="[0->0]" color="red"];
"0:->A(x~u,y~u)" -| "2:A(x~u,y~u)->A(x~u,y~p)" [label="[0->0]" color="red"];
"1:A(x~u,y~u)->A(x~p,y~u)" -| "3:A(x~p,y~u)->D(x~u)" [label="[0->0]" color="red"];
"3:A(x~p,y~u)->D(x~u)" -| "4:D(x~u)->D(x~p)" [label="[0->0]" color="red"];
}
