#Download sources/binaries at https://github.com/Kappa-Dev/KaSim
#
#Command line is: KaSa parallel_bonds.ka --output-directory output --verbosity-level-for-view-analysis Mute --debugging-mode
#
#This file contains the description of the influence map in dot.
#Please use graphviz (http://www.graphviz.org) or OmniGraffle to export it to a PDF
#
digraph G{
"r0" [shape="box" color="lightskyblue"];
"r1" [shape="box" color="lightskyblue"];
"r2" [shape="box" color="lightskyblue"];
"r3" [shape="box" color="lightskyblue"];
"r4" [shape="box" color="lightskyblue"];
"r5" [shape="box" color="lightskyblue"];
"r6" [shape="box" color="lightskyblue"];
"r7" [shape="box" color="lightskyblue"];
"r0" -> "r2" [label="[0->0]" color="green"];
"r0" -> "r6" [label="[0->0]" color="green"];
"r1" -> "r2" [label="[0->1]" color="green"];
"r1" -> "r7" [label="[0->0]" color="green"];
"r2" -> "r5" [label="[0->0];[1->1]" color="green"];
"r3" -> "r4" [label="[0->0];[1->1]" color="green"];
"r6" -> "r0" [label="[0->0]" color="green"];
"r6" -> "r3" [label="[0->0]" color="green"];
"r7" -> "r1" [label="[0->0]" color="green"];
"r7" -> "r3" [label="[0->1]" color="green"];

"r0" -| "r2" [label="[0->0]" color="red"];
"r0" -| "r6" [label="[0->0]" color="red"];
"r1" -| "r2" [label="[0->1]" color="red"];
"r1" -| "r7" [label="[0->0]" color="red"];
"r2" -| "r5" [label="[0->0];[1->1]" color="red"];
"r3" -| "r4" [label="[0->0];[1->1]" color="red"];
"r6" -| "r0" [label="[0->0]" color="red"];
"r6" -| "r3" [label="[0->0]" color="red"];
"r7" -| "r1" [label="[0->0]" color="red"];
"r7" -| "r3" [label="[0->1]" color="red"];
}
