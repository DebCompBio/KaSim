digraph G{ 
node [shape=box, style=filled, fillcolor=lightskyblue];
"E.R" ; 
"E/R" ; 
"R.R" ; 
"R/R" ; 
"R.int" ; 
"R/int" ; 
"obs" ; 
edge [color=green, arrowhead=normal];
"E.R" -> "E/R"[label ="[0->0];[1->1]"] ;
"E.R" -> "R.R"[label ="[1->0];[1->1]"] ;
"E/R" -> "E.R"[label ="[0->0];[1->1]"] ;
"E/R" -> "obs"[label ="[1->0]"] ;
"R.R" -> "R/R"[label ="[0->0];[0->1];[1->0];[1->1]"] ;
"R.R" -> "R.int"[label ="[0->0];[0->1];[1->0];[1->1]"] ;
"R/R" -> "E/R"[label ="[0->1];[1->1]"] ;
"R/R" -> "R.R"[label ="[0->0];[0->1];[1->0];[1->1]"] ;
"R.int" -> "R/int"[label ="[0->0];[1->1]"] ;
"R/int" -> "R/R"[label ="[0->0];[0->1];[1->0];[1->1]"] ;
"R/int" -> "R.int"[label ="[0->0];[0->1];[1->0];[1->1]"] ;
"obs" -> "R/R"[label ="[0*->0];[0*->1]"] ;
