"R/R" -> "R/R"[label ="[0->1];[1->0]"] ; 
"R/R" -> "R.int"[label ="[0->0];[0->1];[1->0];[1->1]"] ; 
"R.int" -> "R/R"[label ="[0->0];[0->1];[1->0];[1->1]"] ; 
"R.int" -> "R.int"[label ="[0->1];[1->0]"] ; 
"R/int" -> "obs"[label ="[0->0];[1->0]"] ; 
"obs" -> "R/int"[label ="[0*->0];[0*->1];[0->0];[0->1]"] ; 
"obs" -> "obs"[label ="[0*->0]"] ; 

