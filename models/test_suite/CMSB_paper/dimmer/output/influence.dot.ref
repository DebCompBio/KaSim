edge [color=red, arrowhead=tee];
"E.R" -> "obs"[label ="[1->0]"] ; 
"E/R" -> "R.R"[label ="[1->0];[1->1]"] ; 
"R.R" -> "E/R"[label ="[0->1];[1->1]"] ; 
