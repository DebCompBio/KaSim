%agent: E(r)
%agent: R(e,r,s)

E(r),R(e) -> E(r!1),R(e!1) @ 1
E(r!1),R(e!1,r) -> E(r),R(e,r) @ 2
R(e!_,r),R(e!_,r) -> R(e!_,r!1),R(e!_,r!1) @ 3
R(e!_,r),R(e!_,s) -> R(e!_,r!1),R(e!_,s!1) @ 3
R(r!_) -> @ 4
R(r!_) -> R(r) @ 5

%obs: 'dimmers' |R(r!_)|/2
%obs: 'monomers' |R(r)|

%init: 10 E(r)
%init: 100 R(r,e)
