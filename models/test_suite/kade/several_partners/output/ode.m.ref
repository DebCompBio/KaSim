function main=main()
%% command line: 
%%     'KaDE' 'several_partners.ka' '-d' 'output' '-t' '1' '-p''100'
%% THINGS THAT ARE KNOWN FROM KAPPA FILE AND KaSim OPTIONS;
%% 
%% init - the initial abundances of each species and token
%% tinit - the initial simulation time (likely 0)
%% tend - the final simulation time 
%% initialstep - initial time step at the beginning of numerical integration
%% num_t_point - the number of time points to return
%%
%% variables (init(i),y(i)) denote numbers of embeddings 
%% rule rates are not corrected by the number of automorphisms in the lhs of rules


tinit=0.000000;
tend=1.000000;
initialstep=0.000001;
num_t_point=100;

global nodevar
nodevar=6;
nvar=3;
nobs=0;
nrules=4;

global var
var=zeros(nvar,1);
global init
init=sparse(nodevar,1);

t = 0.000000;

init(6)=t;
init(1)=(1*6); %B(x, y)
init(2)=(1*4.500000); %A(x)
var(1)=((1*(((init(2)+init(3))+init(4))+(2*init(5))))*t);

global k
global kd
global kun
global kdun

k(1)=1; %A(x), B(x) -> A(x!1), B(x!1)

uiIsOctave = false;
uiIsMatlab = false;
LIC = license('inuse');
for elem = 1:numel(LIC)
    envStr = LIC(elem).feature
    if strcmpi(envStr,'octave')
       LICname=envStr;
       uiIsOctave = true;
       break
    end
    if strcmpi(envStr,'matlab')
       LICname=envStr
       uiIsMatlab = true;
       break
    end
end


options = odeset('RelTol', 1e-3, ...
                 'AbsTol', 1e-3, ...
                 'InitialStep', initialstep, ...
                 'MaxStep', tend);


if uiIsMatlab
   soln =  ode15s(@ode_aux,[tinit tend],ode_init(),options);
   soln.y=soln.y';
elseif uiIsOctave
   soln = ode2r(@ode_aux,[tinit tend],ode_init(),options);
end


nrows = length(soln.x);
tmp = zeros(nodevar,1);

t = linspace(tinit, tend, num_t_point+1);
obs = zeros(nrows,nobs);

for j=1:nrows
    for i=1:nodevar
        z(i)=soln.y(j,i);
    end
    h=ode_obs(z);
    for i=1:nobs
        obs(j,i)=h(i);
    end
end
if nobs==1
   y = interp1(soln.x, obs, t, 'pchip')';
else
   y = interp1(soln.x, obs, t, 'pchip');
end


filename = 'data.out';
fid = fopen (filename,'w');
fprintf(fid,'# KaDE several_partners.ka -d output -t 1 -p 100\n')
fprintf(fid,'# t')
fprintf(fid,'\n')
for j=1:num_t_point+1
    fprintf(fid,'%f',t(j));
    for i=1:nobs
        fprintf(fid,' %f',y(j,i));
    end
    fprintf(fid,'\n');
end
fclose(fid);


end



function dydt=ode_aux(t,y)

global nodevar
global var
global k
global kd
global kun
global kdun

var(1)=((1*(((y(2)+y(3))+y(4))+(2*y(5))))*t);

k(2)=t;
k(3)=var(1);
k(4)=var(1);

dydt=zeros(nodevar,1);

%rule    : A(x!_) -> 
%reaction: A(x!1), B(x!1, y) -> B(x, y)

dydt(3)=dydt(3)-k(3)*y(3);
dydt(1)=dydt(1)+k(3)*y(3);

%rule    : A(x), B(y) -> A(x!1), B(y!1)
%reaction: A(x!1), B(x!1, y) + A(x) -> A(x!1), B(x!1, y!2), A(x!2)

dydt(2)=dydt(2)-k(2)*y(2)*y(3);
dydt(3)=dydt(3)-k(2)*y(2)*y(3);
dydt(5)=dydt(5)+k(2)*y(2)*y(3);

%rule    : A(x!_) -> 
%reaction: A(x!1), B(x!1, y!2), A(x!2) -> A(x!1), B(x!1, y)

dydt(5)=dydt(5)-k(4)*y(5);
dydt(3)=dydt(3)+k(4)*y(5);

%rule    : A(x!_) -> 
%reaction: A(x!1), B(x!1, y!2), A(x!2) -> A(x!1), B(x, y!1)

dydt(5)=dydt(5)-k(3)*y(5);
dydt(4)=dydt(4)+k(3)*y(5);

%rule    : A(x!_) -> 
%reaction: A(x!1), B(x, y!1) -> B(x, y)

dydt(4)=dydt(4)-k(4)*y(4);
dydt(1)=dydt(1)+k(4)*y(4);

%rule    : A(x), B(x) -> A(x!1), B(x!1)
%reaction: A(x!1), B(x, y!1) + A(x) -> A(x!1), B(x!1, y!2), A(x!2)

dydt(2)=dydt(2)-k(1)*y(2)*y(4);
dydt(4)=dydt(4)-k(1)*y(2)*y(4);
dydt(5)=dydt(5)+k(1)*y(2)*y(4);

%rule    : A(x), B(y) -> A(x!1), B(y!1)
%reaction: B(x, y) + A(x) -> A(x!1), B(x, y!1)

dydt(2)=dydt(2)-k(2)*y(2)*y(1);
dydt(1)=dydt(1)-k(2)*y(2)*y(1);
dydt(4)=dydt(4)+k(2)*y(2)*y(1);

%rule    : A(x), B(x) -> A(x!1), B(x!1)
%reaction: B(x, y) + A(x) -> A(x!1), B(x!1, y)

dydt(2)=dydt(2)-k(1)*y(2)*y(1);
dydt(1)=dydt(1)-k(1)*y(2)*y(1);
dydt(3)=dydt(3)+k(1)*y(2)*y(1);
dydt(6)=1;

end


function Init=ode_init()

global nodevar
global init
Init=zeros(nodevar,1);

Init(1) = init(1); %B(x, y)
Init(2) = init(2); %A(x)
Init(3) = init(3); %A(x!1), B(x!1, y)
Init(4) = init(4); %A(x!1), B(x, y!1)
Init(5) = init(5); %A(x!1), B(x!1, y!2), A(x!2)
Init(6) = init(6); %t
end


function obs=ode_obs(y)

global nobs
global var
obs = zeros(nobs,1);

t = y(6);
var(1)=((1*(((y(2)+y(3))+y(4))+(2*y(5))))*t);


end


main();
