begin model
begin parameters
    Lig_tot  6.0e3
    Rec_tot  4.0e2


     kp1  1.32845238e-7
     km1  0


end parameters

begin seed species
    Lig(l,l,l)         Lig_tot
    Rec(a,b~Y,g~Y)     Rec_tot
end seed species

begin reaction rules
    # Ligand-receptor binding
    Rec(a) + Lig(l,l) <-> Rec(a!1).Lig(l!1,l) kp1, km1

end reaction rules
end model

## actions ##
generate_network({overwrite=>1,max_iter=>100})
